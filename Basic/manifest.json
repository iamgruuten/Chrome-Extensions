{
    "manifest_version":2,
    "name": "Overflow Extension",
    "version" : "1",
    "web_accessible_resources":[
        "Images/cat.jpg"
    ],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["kitten.js"]            
        }
    ],
    "background":{
        "scripts":["background.js"]
    },
    "icons":{
        "128": "images/icon128.png",
        "48": "images/icon48.png",
        "16": "images/icon16.png"

    },
    "browser_action":{
        "default_icon": "images/icon16.png"
    }
}